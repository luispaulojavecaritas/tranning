<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="gesadminPU"
		transaction-type="JTA">
		<provider> org.eclipse.persistence.jpa.PersistenceProvider </provider>
		<jta-data-source>jdbc_GesadminPool</jta-data-source>
		<class>pe.com.gesadmin.entity.Bloque</class>
		<class>pe.com.gesadmin.entity.ContactoProveedor</class>
		<class>pe.com.gesadmin.entity.Egreso</class>
		<class>pe.com.gesadmin.entity.Ingreso</class>
		<class>pe.com.gesadmin.entity.Orden</class>
		<class>pe.com.gesadmin.entity.Pais</class>
		<class>pe.com.gesadmin.entity.Persona</class>
		<class>pe.com.gesadmin.entity.Proveedor</class>
		<class>pe.com.gesadmin.entity.Puesto</class>
		<class>pe.com.gesadmin.entity.PuestoPersona</class>
		<class>pe.com.gesadmin.entity.Sexo</class>
		<class>pe.com.gesadmin.entity.SituacionOrden</class>
		<class>pe.com.gesadmin.entity.TipoDocumento</class>
		<class>pe.com.gesadmin.entity.TipoEstatus</class>
		<class>pe.com.gesadmin.entity.TipoOrden</class>
		<class>pe.com.gesadmin.entity.Usuario</class>
		<class>pe.com.gesadmin.entity.Periodo</class>
		<class>pe.com.gesadmin.entity.Movimiento</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<shared-cache-mode>NONE</shared-cache-mode>
	</persistence-unit>
</persistence>
